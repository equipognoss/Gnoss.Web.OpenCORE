@*
Descripción
----------------------------------------------------------------
Vista parcial donde donde se mostrará el panel lateral izquierdo para la navegación en la Administración (DevTools)

Variables
----------------------------------------------------------------
*@
@{
    CommunityModel Comunidad = Html.GetComunidad();
    UserIdentityModel IdentidadActual = Html.GetIdentidadActual();
    // Nombre de la sección en la que se encuentra el usuario
    AdministracionSeccionesDevTools.SeccionesDevTools seccionDevTools = Html.GetActiveSeccionDevTools();
    // Nombre de la subsección en la que se encuentra el usuario
    AdministracionSeccionesDevTools.SubSeccionesDevTools subSeccionDevTools = Html.GetActiveSubSeccionDevTools();
    // Permisos de páginas
    PermisosPaginasAdministracionViewModel permisosPaginas = ViewBag.PermisosPaginas;
    // Permisos para administrar aspectos relacionados con el diseño de la comunidad
    bool administrarDiseño = false;
    List<PermisoComunidad> permisosAdministracion = ViewBag.ListaPermisosAdministracionIdentidad;
    List<PermisoContenidos> permisosContenidos = ViewBag.ListaPermisosContenidosIdentidad;
    List<PermisoEcosistema> permisosEcosistema = ViewBag.ListaPermisosAdministracionEcosistemaUsuario;
    // Permisos para administrar aspectos relacionados con el diseño de páginas
    // Comunidad    
    bool administrarInformacionGeneral = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarInformacionGeneral);
    //bool administrarUtilidades = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarTiposDeContenidosYPermisos);
    bool administrarUtilidades = false; // esta pagina va a desaparecer
    bool administrarCategorias = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.VerCategorias) || permisosContenidos.Contains(PermisoContenidos.EliminarCategoria) || permisosContenidos.Contains(PermisoContenidos.ModificarCategoria) || permisosContenidos.Contains(PermisoContenidos.AnyadirCategoria));
    bool administrarInteraccionSocial = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarInteraccionesSociales);
    bool administrarMiembros = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarMiembros);
    bool administrarSolicitudes = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarSolicitudesDeAccesoAGrupo);
    bool administrarNivelesCertificacion = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarNivelesDeCertificacion);
    bool administrarRoles = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarRolesYPermisos);

    // Estructura
    bool administrarPaginas = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.VerPagina) || permisosContenidos.Contains(PermisoContenidos.EditarPagina) || permisosContenidos.Contains(PermisoContenidos.EliminarPagina) || permisosContenidos.Contains(PermisoContenidos.CrearPagina) || permisosContenidos.Contains(PermisoContenidos.PublicarPagina));
    bool administrarPesos = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarPesosAutocompletado);
    bool administrarRedirecciones = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarRedirecciones);
    bool administrarCMS = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.VerComponenteCMS) || permisosContenidos.Contains(PermisoContenidos.EditarComponenteCMS) || permisosContenidos.Contains(PermisoContenidos.EliminarComponenteCMS) || permisosContenidos.Contains(PermisoContenidos.CrearComponenteCMS));
    bool administrarMultimediaCMS = permisosContenidos != null && permisosContenidos.Contains(PermisoContenidos.GestionarMultimediaCMS);

    // Configuracion general
    bool administrarOAuth = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.DescargarConfiguracionOAuth);
    bool administrarCookies = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarCookies);
    bool administrarFTP = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AccederAlFTP);
    bool administrarTraducciones = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarTraducciones);
    bool administrarDatosExtra = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarDatosExtraRegistro);
    bool administrarTrazas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarTrazas);
    bool administrarDescargar = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarConfiguraciones);
    bool administrarCache = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarCache);
    bool administrarSEO = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AdministrarSEOYGoogleAnalytics);
    bool administrarEstadisticas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AccederAEstadisticasDeLaComunidad);
    bool administrarClausulas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarClausulasDeRegistro);
    bool administrarBuzonCorreo = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarBuzonDeCorreo);
    bool administrarServiciosExternos = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarServiciosExternos);
    bool administrarEstadoSistemas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AccederAlEstadoDeLosServicios);
    bool administrarOpcionesMetaAdmin = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarOpcionesDelMetaadministrador);
    bool administrarEventos = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarEventosExternos);

    // Grafo de conocimiento
    bool administrarOC = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.ModificarValorEntidadSecundaria) || permisosContenidos.Contains(PermisoContenidos.EditarPagina) || permisosContenidos.Contains(PermisoContenidos.EliminarValorEntidadSecundaria) || permisosContenidos.Contains(PermisoContenidos.AnyadirValorEntidadSecundaria) || permisosContenidos.Contains(PermisoContenidos.GestionarOC));
    bool administrarTesauros = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.AnyadirValorTesauro) || permisosContenidos.Contains(PermisoContenidos.VerTesauroSemantico) || permisosContenidos.Contains(PermisoContenidos.EliminarValorTesauro) || permisosContenidos.Contains(PermisoContenidos.ModificarValorTesauro));
    bool administrarSPARQL = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AccesoSparqlEndpoint);
    bool administrarConsultasCargaMasiva = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.ConsultarCargasMasivas);
    bool administrarBorradoMasivo = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.EjecutarBorradoMasivo);

    // Descubrimiento y analisis
    bool administrarFacetas = permisosContenidos != null && (permisosContenidos.Contains(PermisoContenidos.VerFaceta) || permisosContenidos.Contains(PermisoContenidos.ModificarFaceta) || permisosContenidos.Contains(PermisoContenidos.EliminarFaceta) || permisosContenidos.Contains(PermisoContenidos.CrearFaceta));
    bool administrarSugerencias = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarSugerenciasDeBusqueda);
    bool administrarContextos = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarInformacionContextual);
    bool administrarParametrosBusqueda = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarParametrosDeBusquedaPersonalizados);
    bool administrarMapa = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarMapa);
    bool administrarGraficos = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.AdministrarGraficos);

    // Apariencia
    bool administrarVistas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarVistas);

    // IC
    bool administrarIC = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarIntegracionContinua);

    // Mantenimiento
    bool administrarReprocesar = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.EjecutarReprocesadosDeRecursos);

    // Aplicaciones especificas
    bool administrarAplicacionesEspecificas = permisosAdministracion != null && permisosAdministracion.Contains(PermisoComunidad.GestionarAplicacionesEspecificas);


    // Ecosistema
    // Configuracion general
    bool administrarTraduccionesEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarTraduccionesEcosistema);
    bool administrarDatosExtraEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarDatosExtraRegistroEcosistema);
    bool administrarBuzonCorreoEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarBuzonDeCorreoEcosistema);
    bool administrarEventosExternosEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarEventosExternosEcosistema);
    bool administrarConfiguracion = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarLaConfiguracionPlataforma);
    bool administrarSharepoint = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.ConfiguracionDeSharePoint);
    bool administrarMiembrosEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.AdministrarMiembrosEcosistema);

    bool administrarVistasEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarVistasEcosistema);
    bool administrarCategoriasPlataforma = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarCategoriasDePlataforma);
    bool administrarICEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.AdministrarIntegracionContinua);
    bool administrarSolicitudesComunidad = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.AdministrarSolicitudesComunidad);
    bool administrarRolesEcosistema = permisosEcosistema != null && permisosEcosistema.Contains(PermisoEcosistema.GestionarRolesYPermisosEcosistema);

    // Controlar visualización del botón para documentación
    bool showDocumentationByDefault = !string.IsNullOrEmpty(ViewBag.showDocumentationByDefault) ? (bool.Parse(ViewBag.showDocumentationByDefault)) : false;
    // Controlar si está en Administración de Plataforma (No visualizar otras opciones de configuración)
    // bool isInAdminPlatform = !string.IsNullOrEmpty(ViewBag.isInAdminPlatform) ? ( bool.Parse(ViewBag.isInAdminPlatform)) : false;
    // Controlar si se está en Administración del Ecosistema (No visualizar otras opciones de configuración)
    bool isInEcosistemaPlatform = !string.IsNullOrEmpty(ViewBag.isInEcosistemaPlatform) ? (bool.Parse(ViewBag.isInEcosistemaPlatform)) : false;
    // Posibilidad de NO mostrar el panel lateral izquierdo de las DevTools
    bool hideLeftNavigationPanel = !string.IsNullOrEmpty(ViewBag.HideLeftNavigationPanel) ? (bool.Parse(ViewBag.HideLeftNavigationPanel)) : false;
    // Url para documentación
    string urlSeccionDevToolsDocumentation = "";
    // Conocer los permisos del usuario para administrar una comunidad
    CommunityModel.PermissionsModel permisosComunidad = Comunidad.Permissions;

    @* Controlar la urlDocumentación *@
    if (seccionDevTools != AdministracionSeccionesDevTools.SeccionesDevTools.Home && seccionDevTools != AdministracionSeccionesDevTools.SeccionesDevTools.Documentacion)
    {
        urlSeccionDevToolsDocumentation = $"{Comunidad.Url}/{Html.GetText("URLSEM", "ADMINISTRARDOCUMENTACIONCOMUNIDAD")}?section={seccionDevTools}";
    }
    else if (seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Home || showDocumentationByDefault == false)
    {
        showDocumentationByDefault = false;
    }
}
@*
----------------------------------------------------------------

-------------------- Panel lateral de navegación de Administración/DevTools --------------------*@


@* Menu de navegación lateral izquierdo *@

@{
    if (!hideLeftNavigationPanel)
    {
        <aside id="panel-lateral" class="panel-lateral">

            @* Botón para mostrar/ocultar el panel lateral *@
            <span id="btnTogglePanelLateral" class="material-icons btn-toogle-panel-lateral">
                chevron_left
            </span>

            <div class="header">
                @* Logo de la comunidad *@
                @if (!String.IsNullOrEmpty(Comunidad.Logo))
                {
                    <div class="logo">
                        <img src="@Comunidad.Logo" />
                    </div>
                }
                <p id="header-title"><a href="@Comunidad.Url">@Comunidad.Name</a></p>
            </div>

            @* Buscador de secciones *@
            <div class="form-group mb-3 header-searcher">
                <div class="filtroRapido">
                    <div class="input-container">
                        <input id="input-header-searchable" class="form-control" type="text" placeholder="@Html.GetText("DEVTOOLS","BUSCARPALABRASCLAVE")" autocomplete="off">

                        <div id="header-searcher-loading" class="spinner-border spinner-border-sm d-none" role="status">
                            <span class="sr-only">Cargando...</span>
                        </div>
                    </div>
                    <span id="input-header-searchable-icon" class="icon material-icons">search</span>
                </div>
            </div>

            <div class="body custom-css-scroll">
                <div id="menu">

                    @* Sección Home, Comunidad y Estructura (Comunidad) *@
                    @if (!isInEcosistemaPlatform)
                    {
                        @* Sección Home *@
                        <a id="home" href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARCOMUNIDADHOME")" class="@((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Home) ? "activo" : null) ">
                            <span class="material-icons" alt="Home" title="Home" aria-label="Home">home</span>
                            Home
                        </a>
                        @* Sección Comunidad *@
                        <div class="group-collapse comunidad">
                            <a href="#comunidad"
                            data-toggle="collapse"
                            aria-expanded="@((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Comunidad) ? "true" : "false" )"
                            class="parent-navigation @((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Comunidad) ? "activo" : null) ">
                                <span class="material-icons" alt="@Html.GetText("ADMINISTRACIONSEMANTICA","COMUNIDAD")" title="@Html.GetText("ADMINISTRACIONSEMANTICA","COMUNIDAD")" aria-label="@Html.GetText("ADMINISTRACIONSEMANTICA","COMUNIDAD")">language</span>
                                <span class="texto">@Html.GetText("ADMINISTRACIONSEMANTICA", "COMUNIDAD")</span>
                                @* Arrow down/up dependiendo (Activo /No activo) *@
                                <span class="material-icons icon-collapse">@((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Comunidad) ? "keyboard_arrow_up" : "keyboard_arrow_down")</span>
                            </a>
                            <div id="comunidad" class="collapse @((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Comunidad) ? "show" : null) ">
                                <ul class="pmd-sidebar-nav">
                                    @* Administrar Comunidad General *@
                                    @if (administrarInformacionGeneral)
                                    {
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARCOMUNIDADGENERAL")"
                                            data-searchable="Estado de la comunidad, cambiar nombre corto de la comunidad, tipo de acceso de la comunidad, nombre de la comunidad, descripción de la comunidad, imágenes de la comunidad, etiquetas de la comunidad, idiomas de la comunidad, multiidioma, multilingue"
                                            class="info-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_InformacionGeneral) ? "activo" : null)">
                                                @Html.GetText("COMADMINCOMUNIDAD", "ADMINISTRARCOMUNIDADGENERAL")
                                            </a>
                                        </li>                                        
                                    }
                                    @if (administrarUtilidades)
                                    {
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARCOMUNIDADUTILIDADES")"
                                               data-searchable="descarga de adjuntos, permisos, permisos a grupos"
                                               class="permisos-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_TiposDeContenidoPermisos) ? "activo" : null)">
                                                @Html.GetText("DEVTOOLS", "TIPOSDECONTENIDO/PERMISOS")
                                            </a>
                                        </li>
                                    }
                                    @if (administrarCategorias)
                                    {
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARCATEGORIASCOM")"
                                            data-searchable="Categorías, crear categoria, editar categoria, borrar categoria, eliminar categoria, modificar categoría"
                                            class="categorias-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Categorias) ? "activo" : null)">
                                                @Html.GetText("COMADMINCOMUNIDAD", "ADMINISTRARCATEGORIASCOM")
                                            </a>
                                        </li>
                                    }
                                    @if (administrarInteraccionSocial)
                                    {
                                        @* Interacción social *@
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARINTERACCIONSOCIAL")"
                                            data-searchable="Invitaciones, Votaciones, votaciones negativas, Comentarios disponibles, Permitir a los supervisores administrar los grupos, Los usuarios pueden compartir recursos de esta comunidad a otras comunidades, Los usuarios pueden crear recursos privados en la comunidad"
                                            class="interaccion-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_InteraccionSocial) ? "activo" : null)">
                                                @Html.GetText("DEVTOOLS", "INTERACCIONSOCIAL")
                                            </a>
                                        </li>
                                    }
                                    @if(administrarMiembros)
                                    {
                                        @* Miembros  *@
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "COMADMINCOMMIEMBROS")"
                                            data-searchable="miembros, administrar miembros, usuarios, administrar usuarios"
                                            class="miembros-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Miembros) ? "activo" : null)">
                                                @Html.GetText("COMADMINCOMUNIDAD", "COMADMINCOMMIEMBROS")
                                            </a>
                                        </li>
                                    }



                                    @* Solicitudes de acceso a la comunidad - Restringidas *@
                                    @if (permisosComunidad.ManageRequestAccess)
                                    {
                                        int numRequestAccess = Comunidad.SolicitudesPendientes;
                                        string classNewManageRequest = "";
                                        if (numRequestAccess > 0) { classNewManageRequest = "nuevos"; }

                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARSOLICITUACCESOPROYECTO")"
                                            data-searchable="solicitudes de acceso, acceso a la comunidad, acceso miembros"
                                            class="solicitudes-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Solicitudes) ? "activo" : null)">
                                                @Html.GetText("SOLICITUDESACCESOPROYECTO", "TITULOPAGINA")
                                            </a>
                                        </li>
                                    }

                                    @* Solicitudes de acceso a grupos *@
                                    @if (administrarSolicitudes)
                                    {
                                        int numRequestGroups = Comunidad.SolicitudesPendientes;
                                        string classNewManageRequestGroup = "";
                                        if (numRequestGroups > 0) { classNewManageRequestGroup = "nuevos"; }
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "GRUPOS")/@Html.GetText("URLSEM", "ADMINISTRARSOLICITUDES")"
                                            data-searchable="solicitudes de acceso, acceso a la comunidad, acceso miembros"
                                            class="solicitudes-comunidad-grupos @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Solicitudes_Grupos) ? "activo" : null)">
                                                @Html.Raw(@Html.GetText("QUEPUEDOHACER", "SOLICITUDESPERTENECERGRUPO",""))
                                            </a>
                                        </li>
                                    }

                                    @if (administrarNivelesCertificacion)
                                    {
                                        @* Niveles de certificación *@
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARCOMUNIDADCERTIFICADOS")"
                                            data-searchable="Niveles de certificación de la comunidad, Activar niveles de certificación de la comunidad,"
                                            class="certificacion-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_NivelesDeCertificacion) ? "activo" : null)">
                                                @Html.GetText("DEVTOOLS", "NIVELESDECERTIFICACION")
                                            </a>
                                        </li>
                                    }


                                    @* Roles *@
                                    @if (!isInEcosistemaPlatform && administrarRoles)
                                    {
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARROLES")"
                                            data-searchable="roles, permisos"
                                            class="roles-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Roles) ? "activo" : null)">
                                                @Html.GetText("DEVTOOLS", "ROLESYPERMISOS")
                                            </a>
                                        </li>
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINLISTADOPERMISOS")"
                                               data-searchable="roles, permisos"
                                               class="roles-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_Permisos) ? "activo" : null)">
                                                @Html.GetText("DEVTOOLS", "LISTADOPERMISOS")
                                            </a>
                                        </li>
                                    }

                                    @* Integración con redes sociales *@
                                    @*
                                    <li>
                                        <a href="javascript: void(0);"
                                        data-searchable="redes sociales, sociales, redes, twitter, facebook"
                                        class="redes-sociales-comunidad @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Comunidad_IntegracionRedesSociales) ? "activo" : null)">
                                            @Html.GetText("DEVTOOLS","INTEGRACIONCONREDESSOCIALES")
                                        </a>
                                    </li>
                                    *@

                                </ul>
                            </div>
                        </div>

                        @* Sección Estructura *@
                        <div class="group-collapse estructura">
                            <a href="#estructura"
                            data-toggle="collapse"
                            aria-expanded="@((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Estructura) ? "true" : "false" )"
                            class="parent-navigation @((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Estructura) ? "activo" : null) ">
                                <span class="material-icons" alt="@Html.GetText("DEVTOOLS","ESTRUCTURA")" title="@Html.GetText("DEVTOOLS","ESTRUCTURA")" aria-label="@Html.GetText("DEVTOOLS","ESTRUCTURA")">folder</span>
                                <span class="texto">@Html.GetText("DEVTOOLS", "ESTRUCTURA")</span>
                                @* Arrow down/up dependiendo (Activo /No activo) *@
                                <span class="material-icons icon-collapse">@((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Estructura) ? "keyboard_arrow_up" : "keyboard_arrow_down")</span>
                            </a>

                            <div id="estructura" class="collapse @((seccionDevTools == AdministracionSeccionesDevTools.SeccionesDevTools.Estructura) ? "show" : null) ">
                                <ul class="pmd-sidebar-nav">
                                    @if(administrarPaginas)
                                    {
                                        @* Administrar Páginas *@
                                        <li>
                                            <a href="@Comunidad.Url/@Html.GetText("URLSEM", "ADMINISTRARPESTANYASCOM")"
                                            data-searchable="Páginas, paginas, páginas de la comunidad"
                                            class="paginas-estructura @((subSeccionDevTools == AdministracionSeccionesDevTools.SubSeccionesDevTools.Estructura_Paginas) ? "activo" : null)">
                                                @Html.GetText("COMADMINCOMUNIDAD", "ADMINISTRARPESTANYASCOM")
                                            </a>
                                        </li>
