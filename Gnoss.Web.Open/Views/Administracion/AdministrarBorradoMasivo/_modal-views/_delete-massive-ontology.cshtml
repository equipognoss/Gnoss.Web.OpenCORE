@*
Descripción
-----------------------------------------------
Vista modal para confirmar la eliminación masiva de recursos de una o varias ontologías seleccionadas


Variables
----------------------------------------------------------------
*@
@{

}

<div id="modalMassiveOntologyDelete"
     class="modal modal-top fade modal-edicion" 
     tabindex="-1" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                @* Título del Modal *@
                <p class="modal-title">
                    <span class="material-icons">delete</span>
		            @Html.GetText("COMMON","ELIMINAR")
                </p>
                @* Cierre del Modal *@
                <span class="material-icons cerrar"
                      data-dismiss="modal"
                      aria-label="Close">close</span>
            </div>
            <div class="modal-body">                
                <div class="form-group">
                    <label class="control-label">@Html.GetText("DEVTOOLS","DESEAELIMINARLOSRECURSOSDELASONTOLOGIASSELECCIONADAS")</label>
                </div>
                <div class="form-actions">
                    <button data-dismiss="modal" class="btn btn-outline-primary">@Html.GetText("COMMON","NO")</button>
                    <button id="btnDeleteConfirm" class="btn btn-primary">@Html.GetText("COMMON","SI")</button>
                </div>                
            </div>
        </div>
    </div>
</div>



