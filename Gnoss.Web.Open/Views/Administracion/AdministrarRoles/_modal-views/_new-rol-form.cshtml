@model AdministrarRolesViewModel
@{
    CommunityModel Comunidad = Html.GetComunidad();
    bool esEcosistema = !string.IsNullOrEmpty(ViewBag.isInEcosistemaPlatform) ? (bool.Parse(ViewBag.isInEcosistemaPlatform)) : false;
    string multiIdiomaTabId = @Guid.NewGuid().ToString().Substring(0, 5);
    List<PermisoModel> permisosSeccionComunidad = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionEstructura = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionConfiguracion = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionGrafo = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionDescubrimiento = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionApariencia = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionMantenimiento = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionIC = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionAplicaciones = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionRecursos = new List<PermisoModel>();
    List<PermisoModel> permisosSeccionEcosistema = new List<PermisoModel>();
    if (esEcosistema)
    {
        permisosSeccionEcosistema = Model.ListaPermisos.Where(x => x.Seccion == "ECOSISTEMA").ToList();
    }
    else
    {
        permisosSeccionComunidad = Model.ListaPermisos.Where(x => x.Seccion == "COMUNIDAD").ToList();
        permisosSeccionEstructura = Model.ListaPermisos.Where(x => x.Seccion == "ESTRUCTURA").ToList();
        permisosSeccionConfiguracion = Model.ListaPermisos.Where(x => x.Seccion == "CONFIGURACION").ToList();
        permisosSeccionGrafo = Model.ListaPermisos.Where(x => x.Seccion == "GRAFO").ToList();
        permisosSeccionDescubrimiento = Model.ListaPermisos.Where(x => x.Seccion == "DESCUBRIMIENTO").ToList();
        permisosSeccionApariencia = Model.ListaPermisos.Where(x => x.Seccion == "APARIENCIA").ToList();
        permisosSeccionMantenimiento = Model.ListaPermisos.Where(x => x.Seccion == "MANTENIMIENTO").ToList();
        permisosSeccionIC = Model.ListaPermisos.Where(x => x.Seccion == "IC").ToList();
        permisosSeccionAplicaciones = Model.ListaPermisos.Where(x => x.Seccion == "APLICACIONES").ToList();
        permisosSeccionRecursos = Model.ListaPermisos.Where(x => x.Seccion == "RECURSOS").ToList();
    }
}
<div class="modal-header">
    <p class="modal-title">
        <span class="material-icons">edit</span>@Html.GetText("DEVTOOLS", "CREARNUEVOROL")
    </p>
    <span class="material-icons cerrar" data-dismiss="modal" aria-label="Close">close</span>
</div>
@* Contenido central del modal *@
<div id="modal-body-nuevo-rol" class="modal-body">
    <div class="formulario-edicion">
        <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="informacion-tab" data-toggle="tab" href="#informacion" role="tab" aria-controls="informacion" aria-selected="true">@Html.GetText("DEVTOOLS", "INFORMACION")</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="permisos-tab" data-toggle="tab" href="#permisos" role="tab" aria-controls="permisos" aria-selected="false">@Html.GetText("ROLESYPERMISOS", "PERMISOS")</a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane fade show active" id="informacion" role="tabpanel" aria-labelledby="informacion-tab">
                <div id="panContenidoMultiIdioma" class="contenido-multiIdioma panContenidoMultiIdioma">
                </div>
                <div class="inputsMultiIdioma multiIdiomaContainer" data-idmultiidiomatab="navigation_@multiIdiomaTabId">
                    <div class="form-group mb-4">
                        <input data-labeltext="@Html.GetText("DEVTOOLS", "NOMBREROL")" type="text" name="txtNombreNuevoRol" id="txtNombreNuevoRol" class="form-control multiIdioma">
                    </div>
                    <div class="form-group mb-4">
                        <textarea id="txtDescripcionNuevoRol" rows="6"
                                  name="txtDescripcionNuevoRol"
                                  class="multiIdioma"
                                  data-labeltext="@Html.GetText("DEVTOOLS", "DESCRIPCION")"></textarea>
                    </div>
                </div>
                @if (!esEcosistema)
                {
                    <div class="tab-content pt-2">
                        <div class="form-group mb-4">
                            <label class="control-label d-block">@Html.GetText("DEVTOOLS", "AMBITO")</label>
                            <select id="tipoNuevoRol"
                                    name="tipoNuevoRol"
                                    class="js-select2"
                                    tabindex="0"
                                    aria-hidden="false">
                                <option value="0">@Html.GetText("DEVTOOLS", "TIPOROLCOMUNIDAD")</option>
                                <option value="1">@Html.GetText("DEVTOOLS", "TIPOROLECOSISTEMA")</option>
                            </select>
                            <small class="form-text text-muted">@Html.GetText("ROLESYPERMISOS", "DESCAMBITOCOMUNIDAD")</small>
                        </div>
                    </div>
                }
            </div>
            <div class="tab-pane fade" id="permisos" role="tabpanel" aria-labelledby="permisos-tab">
                <div class="buscador">
                    <form method="post" action="#">
                        <div class="fieldsetGroup searchGroup">
                            <div class="textoBusquedaPrincipalInput">
                                <input type="text" id="txtBusquedaPermisosNuevoRol" class="not-outline text txtBusqueda autocompletar personalizado ac_input" placeholder="@Html.GetText("DEVTOOLS", "ESCRIBEALGO")" autocomplete="off">
                                <span class="botonSearch">
                                    <span class="material-icons">search</span>
                                </span>
                            </div>
                        </div>
                    </form>
                </div>
                @if (esEcosistema)
                {
                    <div id="accordion" class="tabla-permisos">
                        <div class="card">
                            <div class="card-header">
                                <h3 class="mb-0">
                                    <div class="custom-control custom-checkbox">
                                        <input type="checkbox" class="custom-control-input" id="recursos-nuevo-ecosistema">
                                        <label class="custom-control-label" for="recursos-nuevo-ecosistema">@Html.GetText("DEVTOOLS", "TIPOROLECOSISTEMA")</label>
                                    </div>
                                    <button class="btn btn-link"
                                            data-toggle="collapse"
                                            data-target="#permisosecosistema"
                                            aria-expanded="false"
                                            aria-controls="permisosecosistema">                                        
                                        <span class="material-icons">keyboard_arrow_down</span>
                                    </button>
                                </h3>
                            </div>
                            <div id="permisosecosistema"
                                 class="collapse"
                                 data-parent="#accordion">
                                <div class="card-body">
                                    <div class="custom-control__group">
                                    @{
                                        foreach (PermisoModel permiso in permisosSeccionEcosistema)
                                        {
                                            <div class="custom-control custom-checkbox component-wrap permisoecosistema-row">
                                                <span class="component-name" style="display: none;">@Html.GetText("ROLESYPERMISOS", permiso.Nombre.ToUpper())</span>
                                                <input type="checkbox" class="custom-control-input" id="@permiso.Nombre" />
                                                <label class="custom-control-label" for="@permiso.Nombre">@Html.GetText("ROLESYPERMISOS", permiso.Nombre.ToUpper())</label>
                                            </div>
                                        }
                                    }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
