@model ProyectoServicioWeb

<li class="row servicio" id="@Model.Nombre">
    <span class="handle servicio">:::</span>
    <section class="header">
        <span class="nombreServicio" aux="@Html.ObtenerTextoIdiomaUsuario(Model.Nombre, true)">@Html.ObtenerTextoIdiomaUsuario(Model.Nombre, true)</span><span class="tipoServicio"></span>
        <span class="action eliminar">@Html.GetText("COMMON", "ELIMINAR")</span>
        <span class="action editar">@Html.GetText("COMMON", "EDITAR")</span>
    </section>
    @Html.PartialView("_EdicionServicio", Model)
</li>


