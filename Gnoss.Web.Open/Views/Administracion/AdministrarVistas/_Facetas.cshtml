@model ManageViewsViewModel

@*
Descripción
-----------------------------------------------
Vista que carga todas las vistas de tipo Facetas.

Variables
----------------------------------------------------------------
*@

@{

}

@* Pintado de cada Vistas Originales de Facetas *@				
@foreach (string vista in Model.ListOriginalFacetedServiceViews)
{							
	ViewBag.ViewServiceType = "facetas";
	ViewBag.ViewType = "original";
	// Url de la request para las Vistas de tipo Facetas
	ViewBag.UrlActionWeb = Model.UrlActionFacets;
	@Html.PartialView("_partial-views/_originalWebView", vista)								
}

@* Pintado de cada Vistas Editadas de Facetas *@
@foreach (string vista in Model.ListEditedFacetedServiceViews)
{
	ViewBag.ViewServiceType = "facetas";
	ViewBag.ViewType = "personalizada";	
	ViewBag.editedWebView = true;
	// Url de la request para las Vistas de tipo Facetas
	ViewBag.UrlActionWeb = Model.UrlActionFacets;
	@Html.PartialView("_partial-views/_editedWebView", vista)									
}
