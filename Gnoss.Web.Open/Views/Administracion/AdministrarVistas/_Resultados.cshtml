@model ManageViewsViewModel

@*
Descripción
-----------------------------------------------
Vista que carga todas las vistas de tipo Resultado.

Variables
----------------------------------------------------------------
*@

@{

}

@* Pintado de cada Vistas Originales de Resultados *@
@foreach (string vista in Model.ListOriginalResultsServiceViews)
{	
	ViewBag.ViewServiceType = "resultados";
	ViewBag.ViewType = "original";
	// Url de la request para las Vistas de tipo Resultados
	ViewBag.UrlActionWeb = Model.UrlActionResults;
	@Html.PartialView("_partial-views/_originalWebView", vista)								
}


@* Pintado de cada Vistas Editadas de Resultados *@
@foreach (ManageViewsViewModel.EditedView modelo in Model.ListEditedResultsServiceViews)
{
	ViewBag.ViewServiceType = "resultados";
	ViewBag.ViewType = "personalizada";
	ViewBag.editedWebView = true;
	// Url de la request para las Vistas de tipo Resultados
	ViewBag.UrlActionWeb = Model.UrlActionResults;	
	@Html.PartialView("_partial-views/_editedWebView", modelo)									
}
