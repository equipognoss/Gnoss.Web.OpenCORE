@*
Descripción
-----------------------------------------------
Vista modal para confirmar la eliminación de un tesauro seleccionado.

Variables
----------------------------------------------------------------
*@

@{
                      
}

<div id="modal-delete-tesauro" class="modal modal-top fade modal-edicion" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <p class="modal-title">
                    <span class="material-icons">edit</span>
                    @Html.GetText("COMMON", "ELIMINAR")
                </p>
                <span class="material-icons cerrar" data-dismiss="modal" aria-label="Close">close</span>
            </div>
            <div class="modal-body">               
                <div class="form-group">
					<label class="control-label">@Html.GetText("DEVTOOLS","DESEAELIMINARELTESAUROSELECCIONADO")</label>
				</div>

                <div class="form-actions">
                    <button data-dismiss="modal" class="btn btn-outline-primary btnNotConfirmDeleteTesauro">@Html.GetText("COMMON","NO")</button>
                    <button class="btn btn-primary btnConfirmDeleteTesauro">@Html.GetText("COMMON","SI")</button>
				</div>

            </div>
        </div>
    </div>
</div>


